<view class="main">
  <view class="enter">
    <view>
      <view class="num">{{arrNum[0]}}</view>
    </view>
    <view>
      <view class="num">{{arrNum[1]}}</view>
    </view>
    <view>
      <view class="num">{{arrNum[2]}}</view>
    </view>
    <view>
      <view class="num">{{arrNum[3]}}</view>
    </view>
  </view>
  <view class="keyboard">
    <view wx:for="{{[1,2,3,4,5,6,7,8,9,0]}}" wx:key="item" data-num="{{item}}" bindtap="getNum">
      {{item}}
    </view>
    <view bindtap="deleteNum">
      <image src="../../static/icon/backspace.png"></image>
    </view>
  </view>
</view>